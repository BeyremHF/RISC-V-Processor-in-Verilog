#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5bff03249490 .scope module, "test_sign_ext" "test_sign_ext" 2 3;
 .timescale -9 -12;
v0x5bff0329d030_0 .net "imm", 31 0, v0x5bff03249870_0;  1 drivers
v0x5bff0329d120_0 .var "instr", 31 0;
v0x5bff0329d1f0_0 .var "sel", 2 0;
S_0x5bff03249620 .scope module, "dut" "sign_extender" 2 8, 3 2 0, S_0x5bff03249490;
 .timescale -9 -12;
    .port_info 0 /INPUT 32 "instr_full";
    .port_info 1 /INPUT 3 "sel_ext";
    .port_info 2 /OUTPUT 32 "imm_ext";
v0x5bff03249870_0 .var "imm_ext", 31 0;
v0x5bff0329ce10_0 .net "instr_full", 31 0, v0x5bff0329d120_0;  1 drivers
v0x5bff0329cef0_0 .net "sel_ext", 2 0, v0x5bff0329d1f0_0;  1 drivers
E_0x5bff03284750 .event anyedge, v0x5bff0329cef0_0, v0x5bff0329ce10_0;
    .scope S_0x5bff03249620;
T_0 ;
    %wait E_0x5bff03284750;
    %load/vec4 v0x5bff0329cef0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5bff03249870_0, 0, 32;
    %jmp T_0.6;
T_0.0 ;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 31, 6;
    %replicate 20;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 12, 20, 6;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x5bff03249870_0, 0, 32;
    %jmp T_0.6;
T_0.1 ;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 31, 6;
    %replicate 20;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 7, 25, 6;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 5, 7, 4;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x5bff03249870_0, 0, 32;
    %jmp T_0.6;
T_0.2 ;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 31, 6;
    %replicate 19;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 31, 6;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 7, 4;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 6, 25, 6;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 4, 8, 5;
    %concat/vec4; draw_concat_vec4
    %concati/vec4 0, 0, 1;
    %store/vec4 v0x5bff03249870_0, 0, 32;
    %jmp T_0.6;
T_0.3 ;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 31, 6;
    %replicate 11;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 31, 6;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 8, 12, 5;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 1, 20, 6;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 10, 21, 6;
    %concat/vec4; draw_concat_vec4
    %concati/vec4 0, 0, 1;
    %store/vec4 v0x5bff03249870_0, 0, 32;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x5bff0329ce10_0;
    %parti/s 20, 12, 5;
    %concati/vec4 0, 0, 12;
    %store/vec4 v0x5bff03249870_0, 0, 32;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5bff03249490;
T_1 ;
    %pushi/vec4 4289724819, 0, 32;
    %store/vec4 v0x5bff0329d120_0, 0, 32;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5bff0329d1f0_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 19 "$display", "Instruction: %h", v0x5bff0329d120_0 {0 0 0};
    %vpi_call 2 20 "$display", "Bits [31:20]: %b", &PV<v0x5bff0329d120_0, 20, 12> {0 0 0};
    %vpi_call 2 21 "$display", "Bit 31: %b", &PV<v0x5bff0329d120_0, 31, 1> {0 0 0};
    %vpi_call 2 22 "$display", "Extended immediate: %h", v0x5bff0329d030_0 {0 0 0};
    %vpi_call 2 23 "$display", "Expected: ffffffb" {0 0 0};
    %load/vec4 v0x5bff0329d030_0;
    %cmpi/e 4294967291, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 26 "$display", "PASS: Sign extension correct!" {0 0 0};
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 2 28 "$display", "FAIL: Sign extension wrong!" {0 0 0};
T_1.1 ;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
